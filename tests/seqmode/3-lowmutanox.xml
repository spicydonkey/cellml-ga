<?xml version="1.0"?>
<!--3-lowmutanox.xml-->
<!--Testing GA behaviour with low mutation and no crossover-->
<CellMLTimeSeriesFit>
	<GA InitialPopulation="100" MaxPopulation="100" Generations="100" Mutation_proportion="0.05" Crossover_proportion="0.0" Sampling="0">
		<Alleles>
			<Allele Name="x1" LowerBound="-500.0" UpperBound="500.0"/>
			<Allele Name="x2" LowerBound="-500.0" UpperBound="500.0"/>
			<Allele Name="x3" LowerBound="-500.0" UpperBound="500.0"/>
			<Allele Name="x4" LowerBound="-500.0" UpperBound="500.0"/>	
			<Allele Name="x5" LowerBound="-500.0" UpperBound="500.0"/>
		</Alleles>
	</GA>
	<VirtualExperiments>
		<VirtualExperiment TestFunction="schwefel"> 
			<!--TestFunction attribute defines whch test to run GA-->
			<!--Sub-elements in VirtualExperiment are not used by experiment when SEQMODE defined-->
			<AssessmentPoints>
				<AssessmentPoint time="0.0" target="1" />
				<AssessmentPoint time="0.34" target="1" />
				<AssessmentPoint time="1.34" target="1" />
				<AssessmentPoint time="2.0" target="1" />
				<AssessmentPoint time="2.34" target="1" />
				<AssessmentPoint time="3.0" target="1" />
				<AssessmentPoint time="3.34" target="1" />
				<AssessmentPoint time="4.0" target="1" />
				<AssessmentPoint time="4.34" target="1" />
				<AssessmentPoint time="5.0" target="1" />
				<AssessmentPoint time="5.34" target="1" />
			</AssessmentPoints>
		</VirtualExperiment>
	</VirtualExperiments>
</CellMLTimeSeriesFit>

